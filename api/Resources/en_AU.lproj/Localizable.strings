<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Prefix string used in status bar notifications to describe the type of message. -->
	<key>TYPING</key>
	<string>Typing:</string>
	<key>READ</key>
	<string>Read:</string>
	<key>SENDING_FILE</key>
	<string>Sending File:</string>

	<!-- Strings used in status bar notifications to naturally describe the notification. -->
	<key>TYPING_NATURAL</key>
	<string>%@ is typing</string>
	<key>READ_NATURAL</key>
	<string>Read by %@</string>
	<key>SENDING_FILE_NATURAL</key>
	<string>%@ is sending a file</string>
</dict>
</plist>
