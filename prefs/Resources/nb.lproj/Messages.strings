<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Page title. -->
	<key>MESSAGES</key>
	<string>Meldinger</string>

	<!-- Switch in Messages app to enable/disable typing notifications. -->
	<key>SEND_TYPING_NOTIFICATIONS</key>
	<string>Send skriver beskjed</string>

	<!-- Explanation of the typing switch. -->
	<key>DISABLE_TYPING_NOTIFICATIONS_EXPLANATION</key>
	<string>Tillat eller forhindre at mottaker kan se når du skriver.</string>

	<!-- Switch in Messages app to enable/disable read receipt notifications. -->
	<key>SEND_READ_RECEIPTS</key>
	<string>Send lesebekreftelser</string>

	<!-- Explanation of the read receipt switch. -->
	<key>DISABLE_READ_RECEIPTS_EXPLANATION</key>
	<string>Tillat eller forhindre at mottaker kan se når du har lest meldingen deres.</string>

	<!-- Heading for the list of people. -->
	<key>PEOPLE</key>
	<string>Folk</string>

	<!-- Button that allows another person to be added to the list. -->
	<key>ADD_PERSON</key>
	<string>Legg til ny…</string>

	<!-- Button that allows a person to be removed. -->
	<key>REMOVE</key>
	<string>Fjern</string>
</dict>
</plist>
