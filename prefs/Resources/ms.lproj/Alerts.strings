<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Page title. -->
	<key>ALERTS</key>
	<string>Isyarat</string>

	<!-- Button for choosing the alert format. -->
	<key>OVERLAY_FORMAT</key>
	<string>Format Untuk Isyarat</string>

	<!-- Options for the alert format. -->
	<key>OVERLAY_FORMAT_NATURAL</key>
	<string>Normal</string>
	<key>OVERLAY_FORMAT_NATURAL_WITH_EXAMPLE</key>
	<string>Normal: Johnny Appleseed sedang menulis</string>
	<key>OVERLAY_FORMAT_TRADITIONAL</key>
	<string>Biasa</string>
	<key>OVERLAY_FORMAT_TRADITIONAL_WITH_EXAMPLE</key>
	<string>Biasa: Menaip: Johnny Appleseed</string>
	<key>OVERLAY_FORMAT_NAME_ONLY</key>
	<string>Nama Sahaja</string>
	<key>OVERLAY_FORMAT_NAME_ONLY_WITH_EXAMPLE</key>
	<string>Nama Sahaja: Johnny Appleseed</string>

	<!-- Label of switch to enable reduced motion (fade animations). -->
	<key>REDUCE_MOTION</key>
	<string>Kurangkan Pergerakan</string>

	<!-- The label showing how many seconds the user has specified. -->
	<key>DISPLAY_FOR_ONE_SECOND</key>
	<string>1 Saat</string>
	<key>DISPLAY_FOR_N_SECONDS</key>
	<string>%i Saat</string>

	<!-- Button that opens Cydia to the libstatusbar package. -->
	<key>INSTALL_LIBSTATUSBAR</key>
	<string>Memasangkan %@</string>

	<!-- Header for the section shown when libstatusbar isn't installed. -->
	<key>LIBSTATUSBAR_NOT_INSTALLED</key>
	<string>%@ diperlukan bagi menunjuk ikon batang status.</string>

	<!-- Label of button that opens libstatusbar in Cydia. -->
	<key>INSTALL</key>
	<string>Memasangkan</string>

	<!-- The Typing and Read Receipt headings. -->
	<key>TYPING_HEADER</key>
	<string>Menulis</string>
	<key>READ_HEADER</key>
	<string>Penerimaan Bacaan</string>
	<key>SENDING_FILE_HEADER</key>
	<string>Menghantar Fail</string>

	<!-- Buttons shown in the segmented controls for the alert type. -->
	<key>OFF</key>
	<string>Tutup</string>
	<key>ALERT_TYPE_ICON</key>
	<string>Ikon Sahaja</string>
	<key>ALERT_TYPE_OVERLAY</key>
	<string>Bar Status</string>

	<!-- Button for choosing how long the alert shows for. -->
	<key>SHOW_FOR</key>
	<string>Pilihan menunjukkan kepada</string>

	<!-- Options for "Show For". -->
	<key>SHOW_FOR_SPECIFIED</key>
	<string>Tempoh Dinyatakan Saya</string>
	<key>SHOW_FOR_TYPING</key>
	<string>Tempoh Masa Menulis</string>

	<!-- Explanation shown under the "Show For" list. -->
	<key>SHOW_FOR_EXPLANATION</key>
	<string>Untuk menunjukkan isyarat selama tempoh yang dinyatakan pada halaman sebelumnya, pilih "Tempoh Dinyatakan Saya". Untuk menunjukkan isyarat selama mana penghantar sedang menaip, pilih "Tempoh Masa Menulis".</string>

	<!-- Switch to allow or disallow alerts showing in Messages. -->
	<key>HIDE_IN_MESSAGES</key>
	<string>Sumbunyikan Ketika Mesej</string>

	<!-- Buttons to test the alerts. -->
	<key>TEST_TYPING</key>
	<string>Uji Isyarat Menulis</string>
	<key>TEST_READ_RECEIPT</key>
	<string>Uji Isyarat Bacaan Penerimaan</string>
	<key>TEST_SENDING_FILE</key>
 	<string>Isyarat Percubaan Menghantar Fail</string>

 	<!-- Switch to allow or ignore notifications from muted/blocked senders. -->
	<key>IGNORE_DND_SENDERS</key>
	<string>Abaikan Penghantar yang Disenyap/Disekat</string>

	<!-- Explanation under the ignore muted/blocked senders switch. -->
	<key>IGNORE_DND_SENDERS_EXPLANATION</key>
	<string>Jika anda meredam atau menyekat seseorang, TypeStatus tidak akan menunjukkan apa-apa isyarat daripada mereka.</string>
</dict>
</plist>
