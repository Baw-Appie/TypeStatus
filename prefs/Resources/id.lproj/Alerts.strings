<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Page title. -->
	<key>ALERTS</key>
	<string>Tanda</string>

	<!-- Button for choosing the alert format. -->
	<key>OVERLAY_FORMAT</key>
	<string>Format Untuk Sinyal</string>

	<!-- Options for the alert format. -->
	<key>OVERLAY_FORMAT_NATURAL</key>
	<string>Normal</string>
	<key>OVERLAY_FORMAT_NATURAL_WITH_EXAMPLE</key>
	<string>Normal: Johnny Appleseed sedang menulis</string>
	<key>OVERLAY_FORMAT_TRADITIONAL</key>
	<string>Biasa</string>
	<key>OVERLAY_FORMAT_TRADITIONAL_WITH_EXAMPLE</key>
	<string>Biasa: Mengetik: Johnny Appleseed</string>
	<key>OVERLAY_FORMAT_NAME_ONLY</key>
	<string>Nama Saja</string>
	<key>OVERLAY_FORMAT_NAME_ONLY_WITH_EXAMPLE</key>
	<string>Nama Saja: Johnny Appleseed</string>

	<!-- Header for the animation type segmented control. -->
	<key>ANIMATION_TYPE</key>
	<string>Animasi</string>

	<!-- Buttons shown in the segmented control for the animation type. -->
	<key>ANIMATION_SLIDE</key>
	<string>Longsor</string>
	<key>ANIMATION_FADE</key>
	<string>Luntur</string>

	<!-- The label showing how many seconds the user has specified. -->
	<key>DISPLAY_FOR_ONE_SECOND</key>
	<string>1 Detik</string>
	<key>DISPLAY_FOR_N_SECONDS</key>
	<string>%i Detik</string>

	<!-- Title for the alert shown when libstatusbar isn’t installed. -->
	<key>INSTALL_LIBSTATUSBAR</key>
	<string>Pasang %@</string>

	<!-- Body content for the alert shown when libstatusbar isn’t installed. -->
	<key>LIBSTATUSBAR_NOT_INSTALLED</key>
	<string>%@ dibutuhkan untuk menampilkan ikon distatus bar.</string>

	<!-- Label of button that opens libstatusbar in Cydia. -->
	<key>INSTALL</key>
	<string>Pasang</string>

	<!-- The Typing and Read Receipt headings. -->
	<key>TYPING_HEADER</key>
	<string>Mengetik</string>
	<key>READ_HEADER</key>
	<string>Ulasan Penerima</string>
	<key>SENDING_FILE_HEADER</key>
	<string>Mengirim Berkas</string>

	<!-- Buttons shown in the segmented controls for the alert type. -->
	<key>OFF</key>
	<string>Putus</string>
	<key>ALERT_TYPE_ICON</key>
	<string>Ikon Saja</string>
	<key>ALERT_TYPE_OVERLAY</key>
	<string>Status Bar</string>

	<!-- Button for choosing how long the alert shows for. -->
	<key>SHOW_FOR</key>
	<string>Lihat Tarif Untuk</string>

	<!-- Options for "Show For". -->
	<key>SHOW_FOR_SPECIFIED</key>
	<string>Durasi Ditentukan Saya</string>
	<key>SHOW_FOR_TYPING</key>
	<string>Durasi Mengetik</string>

	<!-- Explanation shown under the "Show For" list. -->
	<key>SHOW_FOR_EXPLANATION</key>
	<string>Menunjukkan kepada peringatan untuk durasi yang ditentukan pada halaman sebelumnya, pilih "Durasi Ditentukan Saya". Untuk menunjukkan tanda selama pengirim sedang mengetik, pilih "Durasi Mengetik".</string>

	<!-- Switch to allow or disallow alerts showing in Messages. -->
	<key>HIDE_IN_MESSAGES</key>
	<string>Sembunyikan di Pesan</string>

	<!-- Buttons to test the alerts. -->
	<key>TEST_TYPING</key>
	<string>Percobaan Pemberitahuan Mengetik</string>
	<key>TEST_READ_RECEIPT</key>
	<string>Percobaan Tanda Ulasan Penerimaan</string>
	<key>TEST_SENDING_FILE</key>
	<string>Uji peringatan mengirim berkas</string>

	<!-- Switch to allow or ignore notifications from muted/blocked senders. -->
	<key>IGNORE_DND_SENDERS</key>
	<string>Abaikan yang dibisukan/Pengirim yang diblokir</string>

	<!-- Explanation under the ignore muted/blocked senders switch. -->
	<key>IGNORE_DND_SENDERS_EXPLANATION</key>
	<string>Jika anda membisukan atau memblokir seseorang, TypeStatus tidak akan menampilkan peringatan dari mereka.</string>
</dict>
</plist>
