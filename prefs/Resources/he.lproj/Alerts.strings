<plist version="1.0">
<dict>
	<!-- Page title. -->
	<key>ALERTS</key>
	<string>התרעות</string>

	<!-- Options for the alert format. -->
	<key>OVERLAY_FORMAT_NATURAL</key>
	<string>טבעי</string>
	<key>OVERLAY_FORMAT_NATURAL_WITH_EXAMPLE</key>
	<string>טבעי: פלוני אלמוני מקליד/ה</string>
	<key>OVERLAY_FORMAT_TRADITIONAL</key>
	<string>מסורתי</string>
