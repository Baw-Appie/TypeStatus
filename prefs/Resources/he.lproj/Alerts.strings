	<string>מסורתי: מקליד/ה: פלוני אלמוני</string>
	<key>OVERLAY_FORMAT_NAME_ONLY</key>
	<string>שם בלבד</string>
	<key>OVERLAY_FORMAT_NAME_ONLY_WITH_EXAMPLE</key>
	<string>שם בלבד: פלוני אלמוני</string>

