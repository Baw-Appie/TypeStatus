<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Strings used in status bar notifications to describe the type of message. -->
	<key>TYPING</key>
	<string>Piše:</string>
	<key>READ</key>
	<string>Čita:</string>
	<key>SENDING_FILE</key>
	<string>Slanje datoteke:</string>

	<!-- Strings used in status bar notifications to naturally describe the notification. -->
	<key>TYPING_NATURAL</key>
	<string>%@ piše</string>
	<key>READ_NATURAL</key>
	<string>%@ je pročitao tvoju poruku</string>
	<key>SENDING_FILE_NATURAL</key>
	<string>%@ šalje datoteku</string>
</dict>
</plist>
