<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Page title. -->
	<key>ALERTS</key>
	<string>Alerte</string>

	<!-- Header for the animation type segmented control. -->
	<key>ANIMATION_TYPE</key>
	<string>Animatie</string>

	<!-- Buttons shown in the segmented control for the animation type. -->
	<key>ANIMATION_SLIDE</key>
	<string>Alunecare</string>
	<key>ANIMATION_FADE</key>
	<string>Decolorare</string>

	<!-- Header for the display duration selection. -->
	<key>DISPLAY_DURATION</key>
	<string>Durata Afisare</string>

	<!-- The label showing how many seconds the user has specified. -->
	<key>ONE_SECOND</key>
	<string>O Secunda</string>
	<key>N_SECONDS</key>
	<string>%i Secunde</string>

	<!-- The Typing and Read Receipt headings. -->
	<key>TYPING_HEADER</key>
	<string>Tasteaza</string>
	<key>READ_HEADER</key>
	<string>Citit</string>

	<!-- Buttons shown in the segmented controls for the alert type. -->
	<key>OFF</key>
	<string>Oprit</string>
	<key>ALERT_TYPE_ICON</key>
	<string>Doar Icoana</string>
	<key>ALERT_TYPE_OVERLAY</key>
	<string>Bara Status</string>

	<!-- Button for choosing how long the alert shows for. -->
	<key>SHOW_FOR</key>
	<string>Arata Pentru</string>

	<!-- Options for "Show For". -->
	<key>SHOW_FOR_SPECIFIED</key>
	<string>Durata Specificata</string>
	<key>SHOW_FOR_TYPING</key>
	<string>Durata Cat Se Tasteaza</string>

	<!-- Explanation shown under the "Show For" list. -->
	<key>SHOW_FOR_EXPLANATION</key>
	<string>Pentru a arata alerta pentru durata specificata in pagina precedenta, selecteaza “Durata Specificata”. Pentru a afisa alerta pentru toata durata in care persoana care trimite mesajul, tasteaza, selecteaza “Durata cat se tasteaza”.</string>

	<!-- Switch to allow or disallow alerts showing in Messages. -->
	<key>HIDE_IN_MESSAGES</key>
	<string>Ascunde In Mesaje</string>

	<!-- Buttons to test the alerts. -->
	<key>TEST_TYPING</key>
	<string>Testeaza Alerta Tastare</string>
	<key>TEST_READ_RECEIPT</key>
	<string>Testeaza Alerta Citit</string>
</dict>
</plist>
