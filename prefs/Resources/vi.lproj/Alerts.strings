	<key>TYPING_HEADER</key>
	<string>Đang viết</string>
	<key>READ_HEADER</key>
	<string>Đã đọc</string>

	<!-- Buttons shown in the segmented controls for the alert type. -->
	<key>OFF</key>
	<string>Tắt</string>
	<key>ALERT_TYPE_ICON</key>
	<string>Chỉ riêng biểu tượng</string>
	<key>ALERT_TYPE_OVERLAY</key>
	<string>Thanh Trạng Thái</string>

