<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Strings used in status bar notifications to describe the type of message. -->
	<key>TYPING</key>
	<string>Scrivendo:</string>
	<key>READ</key>
	<string>Leggere:</string>

	<!-- Strings used in status bar notifications to naturally describe the notification. -->
	<key>TYPING_NATURAL</key>
	<string>%@ sta scrivendo</string>
	<key>READ_NATURAL</key>
	<string>Letto da %@</string>
</dict>
</plist>
