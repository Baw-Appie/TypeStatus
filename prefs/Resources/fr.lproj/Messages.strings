<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Page title. -->
	<key>MESSAGES</key>
	<string>Messages</string>

	<!-- Switch in Messages app to enable/disable typing notifications. -->
	<key>SEND_TYPING_NOTIFICATIONS</key>
	<string>Envoyer des notifications lorsque vous écrivez</string>

	<!-- Explanation of the typing switch. -->
	<key>DISABLE_TYPING_NOTIFICATIONS_EXPLANATION</key>
	<string>Permettre ou empêcher le destinataire de voir que vous êtes en train d’écrire.</string>

	<!-- Switch in Messages app to enable/disable read receipt notifications. -->
	<key>SEND_READ_RECEIPTS</key>
	<string>Envoyer des confirmations de lecture</string>

	<!-- Explanation of the read receipt switch. -->
	<key>DISABLE_READ_RECEIPTS_EXPLANATION</key>
	<string>Permettre ou empêcher le destinataire de voir que vous avez lu son message.</string>

	<!-- Heading for the list of people. -->
	<key>PEOPLE</key>
	<string>Personnes</string>

	<!-- Button that allows another person to be added to the list. -->
	<key>ADD_PERSON</key>
	<string>Ajouter…</string>

	<!-- Button that allows a person to be removed. -->
	<key>REMOVE</key>
	<string>Retirer</string>
</dict>
</plist>
